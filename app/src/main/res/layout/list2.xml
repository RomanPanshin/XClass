<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/shedulebuttons"
    >

    <TextView
        android:id="@+id/listView10"
        android:layout_width="404dp"
        android:layout_height="47dp"
        android:fontFamily="@font/montserrat_bold"
        android:text="TextView"
        android:textColor="#3F3D56"
        android:paddingLeft="12dp"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listView11"
        app:layout_constraintVertical_bias="1.0" />
    <TextView

        android:layout_width="66dp"
        android:layout_height="31dp"
        android:fontFamily="@font/montserrat_bold"
        android:paddingLeft="15dp"
        android:text="Урок "
        android:textColor="#3F3D56"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.101"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.031" />
    <TextView
        android:id="@+id/listView11"
        android:layout_width="410dp"
        android:layout_height="44dp"
        android:textColor="#3F3D56"
        android:fontFamily="@font/montserrat_bold"
        android:text="TextView"
        android:paddingLeft="12dp"
        android:textSize="24dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.435" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="110dp"
        android:layout_height="33dp"
        android:layout_marginEnd="360dp"
        android:layout_marginRight="360dp"
        android:fontFamily="@font/montserrat_bold"
        android:paddingLeft="15dp"
        android:text="TextViewNu"
        android:textColor="#3F3D56"
        android:textSize="16dp"
        app:layout_constraintBottom_toTopOf="@+id/listView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtid"
        android:layout_width="110dp"
        android:layout_height="33dp"
        android:fontFamily="@font/montserrat_bold"
        android:paddingLeft="15dp"
        android:text="TextViewNu"
        android:textColor="#3F3D56"
        android:textSize="16dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.976"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.031" />
</androidx.constraintlayout.widget.ConstraintLayout>